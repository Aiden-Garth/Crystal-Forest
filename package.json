  "bugs": {
    "url": "https://github.com/Aiden-Garth/Crystal-Forest/issues"
  },
  "homepage": "https://github.com/Aiden-Garth/Crystal-Forest#readme",
  "devDependencies": {
    "electron": "^3.0.6",
    "electron-packager": "^12.2.0",
    "standard": "^12.0.1"
  }
}
