{
  "name": "project-crystal_forest",
  "productName": "project-crystal_forest",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "pretest": "standard .",
    "test": "standard --fix",
    "prestart": "node main.js",
    "start": "echo Finished.",
    "package-win": "electron-packager . crystal-forest --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Crystal-Forest\"",
    "package-linux": "electron-packager . crystal-forest --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aiden-Garth/Crystal-Forest.git"
  },
